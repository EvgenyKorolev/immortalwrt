{
    "luci-app-belmax": {
        "description": "Grant UCI and RPC access to LuCI BelMax",
        "read": {
            "ubus": {
                "luci.belmax": [ ],
                "file": [ "exec" ]
            },
            "uci": [ "belmax" ]
        },
        "write": {
            "file": {
                "/usr/bin/setup-mesh-main": [ "exec" ],
                "/usr/bin/setup-mesh-node": [ "exec" ]
            },
            "ubus": {
                "luci.belmax": [ ],
                "file": [ "exec" ]
            },
            "uci": [ "belmax" ]
        }
    }
}
